#include <iostream>
#include <cmath>
using namespace std;
 void hardcompare(double a,double b,double c,double d)
 {
 double e=b*log(a);
    double w=d*log(c);
    if(e>w)
    cout<<"YES";
    else
    cout<<"NO";
 }
int main()
{
    double a,b,c,d;
    cin>>a>>b>>c>>d;
 hardcompare(a,b,c,d);
 
    return 0;
}
