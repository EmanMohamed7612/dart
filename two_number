#include<iostream>
#include<iomanip>
#include<cmath>
//#include<bits/stdc++.h>
using namespace std;
void division(float n1,float n2)
{
    	cout << "floor " << n1<< " / " << n2 << " = " << floor(n1/n2)<<"\n";
	cout << "ceil " << n1 << " / " << n2 << " = " <<ceil(n1/n2)<<"\n";
	cout << "round " << n1 << " / " << n2 << " = " << round(n1/n2)<<"\n";
}
int main()
{
 float n1,n2;
	cin >> n1 >> n2;
   division(n1,n2);
}
