// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int sum(int a,int b,int c){
       int sum=0,res=0;
   for(int i=1;i<=a;i++)
   {    int temp=i;
           while(temp>0)
           {
               sum+=temp%10;
               temp/=10;
           }
           if(sum>=b&&sum<=c)
           {
               res+=i;
           }
     sum=0;
   }
   return res;
}
int main() {
   int a,b,c;
   cin>>a>>b>>c;
cout<<sum(a,b,c);
    return 0;
}
