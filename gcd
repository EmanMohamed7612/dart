#include <iostream>
using namespace std;
int gcd(int n,int m)
{
       int i=1,res=1;
       while(i<=n&&m>=i)
   {
       if(n%i==0&&m%i==0)
       {
           res=max(i,res);
       }
       
       i++;
   }
   return res;
}
int main() {
   int n,m;
   cin>>n>>m;
cout<<gcd(n,m);
    return 0;
}
