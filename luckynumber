#include <iostream>
#include <string>
using namespace std;
 
bool isLucky(int number) {
    string numStr = to_string(number);
    for (char ch : numStr) {
        if (ch != '4' && ch != '7') {
            return false;
        }
    }
    return true;
}
 
int main() {
    int a, b;
    cin >> a >> b;
 
    bool found = false;
 
    for (int i = a; i <= b; ++i) {
        if (isLucky(i)) {
            cout << i << " ";
            found = true;
        }
    }
 
    if (!found) {
        cout << "-1";
    }
 
    cout << endl;
    return 0;
}
